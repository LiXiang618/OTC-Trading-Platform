<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Broder" table="broker">
        <id 
			name="id" 
			column="id"
			type="java.lang.String"
			length="45" 
		></id>	
		<property
			name="name"
			column="name"
			update="true"
			insert="true"
			not-null="true"
			type="java.lang.String"
			length="45"
		></property>
    </class>
</hibernate-mapping>